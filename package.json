    "@types/lodash": "^4.14.119",
    "@types/moment-timezone": "^0.5.9",
    "@types/node": "^10.12.12",
    "@types/sleep": "0.0.7",
    "@types/ws": "^6.0.1",
    "axios": "^0.18.0",
    "babel-runtime": "^6.26.0",
    "chalk": "^2.4.1",
    "cli-progress": "^2.1.1",
    "dotenv": "^6.2.0",
    "easy-table": "^1.1.1",
    "jesse-indicators": "^0.1.0",
    "jesse-notifications-telegram": "0.0.1",
    "jsonfile": "^4.0.0",
    "lodash": "^4.17.11",
    "mkdirp": "^0.5.1",
    "moment-timezone": "^0.5.23",
    "redux": "^4.0.1",
    "sleep": "^5.2.3",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.2"
  },
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.5"
